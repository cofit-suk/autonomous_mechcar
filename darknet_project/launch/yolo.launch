<?xml version="1.0" encoding="utf-8"?>

<launch>
  
  <!-- Use YOLOv3 -->
  <arg name="network_param_file"         default="$(find darknet_ros)/config/v3_1.yaml"/>
  <arg name="image" default="/bgr" />


  <!-- Include main launch file -->
  <include file="$(find darknet_ros)/launch/darknet_ros.launch">
    <arg name="network_param_file"    value="$(arg network_param_file)"/>
    <arg name="image" value="$(arg image)" />
  </include>
  <node name="converter" pkg="converter" type="converter"></node>
  <node name="sub" pkg="darknet_project" type="sub" output="screen"></node>
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node"></node>
</launch>
